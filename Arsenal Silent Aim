 local f = game:GetService("\80\108\97\121\101\114\115") local g = game:GetService("\85\115\101\114\73\110\112\117\116\83\101\114\118\105\99\101") local h = game:GetService("\82\101\112\108\105\99\97\116\101\100\83\116\111\114\97\103\101") local b = workspace.CurrentCamera local p = f.LocalPlayer local n = nil getgenv().SilentAimEnabled = true local function m() local s = math.huge local r local a = coroutine.wrap(function() wait(20) s = math.huge end) a() for _, v in next, f:GetPlayers() do if v.Name ~= p.Name then if v.TeamColor ~= p.TeamColor then if workspace:FindFirstChild(v.Name) ~= nil then if workspace[v.Name]:FindFirstChild("\72\117\109\97\110\111\105\100\82\111\111\116\80\97\114\116") ~= nil then if workspace[v.Name]:FindFirstChild("\72\117\109\97\110\111\105\100") ~= nil and workspace[v.Name]:FindFirstChild("\72\117\109\97\110\111\105\100").Health ~= 0 then local k = b:WorldToScreenPoint(workspace[v.Name]:WaitForChild("\72\117\109\97\110\111\105\100\82\111\111\116\80\97\114\116", math.huge).Position) local j = (Vector2.new(g:GetMouseLocation().X, g:GetMouseLocation().Y) - Vector2.new(k.X, k.Y)).Magnitude if j < s then r = v s = j end end end end end end end return r end n = hookmetamethod(game, "\95\95\110\97\109\101\99\97\108\108", function(Self, ...) local e = getnamecallmethod() local l = {...} if not checkcaller() and tostring(Self) == "\72\105\116\80\97\114\116" and tostring(e) == "\70\105\114\101\83\101\114\118\101\114" then if getgenv().SilentAimEnabled == true then l[1] = m().Character.Hitbox end return Self.FireServer(Self, unpack(l)) elseif not checkcaller() and tostring(Self) == "\84\114\97\105\108" and tostring(e) == "\70\105\114\101\83\101\114\118\101\114" then if getgenv().SilentAimEnabled == true then if type(l[1][5]) == "\115\116\114\105\110\103" then l[1][6] = m().Character.Hitbox l[1][2] = m().Character.Hitbox.Position end end return Self.FireServer(Self, unpack(l)) elseif not checkcaller() and tostring(Self) == "\67\114\101\97\116\101\8